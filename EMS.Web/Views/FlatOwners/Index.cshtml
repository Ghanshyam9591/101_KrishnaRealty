@{
    ViewBag.Title = "Flat Owner";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .skin-black .right-side > .content-header {
        background: transparent;
        box-shadow: none;
        margin-left: -232px;
    }
</style>
<aside class="right-side">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Add Party
            <small>Preview</small>
        </h1>
        <div class="breadcrumb">
            <div class="mb-4"> <a href="/FlatOwners/Item/0" class="btn btn-success float-right ml-1">+</a></div>
        </div>
    </section>
</aside>
<br />
<div ng-app="myApp" ng-controller="FlatOwnersList">
    <div style="overflow:auto">
        <table class="table table-hover table-bordered">
            <thead>
                <tr style="background-color:darkgrey">
                    <th>SrNo</th>
                    <th>Owner_Name</th>
                    <th>Owner_Mobile1</th>
                    <th>Owner_Mobile2</th>
                    <th>Owner_Email</th>
                    @*<th>Renter_Name</th>*@
                    @*<th>Renter_Mobile1</th>
                    <th>Renter_Mobile2</th>
                    <th>Renter_Email</th>*@
                    <th>IsActive</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="item in enquiry_data">
                    <td>{{$index+1}}</td>
                    <td><a href="/FlatOwners/Item/{{item.seqid}}">{{item.name}}</a></td>
                    <td>{{item.mobile1}} </td>
                    <td>{{item.mobile2}} </td>
                    <td>{{item.email}}</td>
                    @*<td>{{item.Renter_Name}}</td>
                    <td>{{item.Renter_Mobile1}}</td>
                    <td>{{item.Renter_Mobile2}}</td>
                    <td>{{item.Renter_Email}}</td>*@
                    <td>{{item.is_active}}</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>